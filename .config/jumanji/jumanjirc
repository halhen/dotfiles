set homepage http://familj.k2h.se
set font monospace normal 6
set auto_save 60
set statusbar true
set tabbar true
set single_instance false
set download_dir ~/download
set download_command urxvt -e sh -c "wget --load-cookies ~/.config/jumanji/cookies '%s' -O %s";

searchengine ggl http://www.google.com/search?q=%s

map b nav_history previous 

script ~/.config/jumanji/scripts/dwb2_hints.js
